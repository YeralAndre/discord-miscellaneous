{
  "name": "discord-miscellaneous",
  "version": "1.0.7",
  "description": "Multiple useful functions for your Discord bot.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "discord-miscellaneous",
    "dsc-misc"
  ],
  "author": "Yeral_Andre",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.3.0",
    "ts-node-dev": "^1.1.8",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "axios": "^0.23.0",
    "canvas": "^2.8.0",
    "chalk": "^4.1.0",
    "discord.js": "^13.4.0",
    "mongoose": "^6.0.11",
    "node-canvas-with-twemoji-and-discord-emoji": "^1.2.1",
    "twemoji-parser": "^13.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YeralAndre/discord-miscellaneous.git"
  },
  "bugs": {
    "url": "https://github.com/YeralAndre/discord-miscellaneous/issues"
  },
  "homepage": "https://github.com/YeralAndre/discord-miscellaneous#readme",
  "files": [
    "lib/**/*",
    "fonts/**/*"
  ]
}
